{
  "name": "Misskey",
  "id": "misskey",
  "packaging_format": 1,
  "description": {
    "en": "Microblogging platform"
  },
  "version": "12.47.1~ynh1",
  "url": "https://join.misskey.page/en/",
  "license": "AGPL-3.0-or-later",
  "maintainer": {
    "name": "Anmol"
  },
  "requirements": {
    "yunohost": ">= 3.5"
  },
  "multi_instance": true,
  "services": ["nginx"],
  "arguments": {
    "install": [
      {
        "name": "domain",
        "type": "domain",
        "ask": {
          "en": "Choose a domain name for Misskey",
          "fr": "Choisissez un nom de domaine pour Misskey"
        },
        "example": "example.com"
      },
      {
        "name": "is_public",
        "type": "boolean",
        "ask": {
          "en": "Is it a public application?",
          "fr": "Est-ce une application publique ?"
        },
        "default": true
      }
    ]
  }
}
